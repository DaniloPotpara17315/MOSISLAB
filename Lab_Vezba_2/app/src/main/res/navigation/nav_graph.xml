<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentHome">
    <fragment
        android:id="@+id/fragmentHome"
        android:name="elfak.mosis.myplaces.FragmentHome"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_fragmentHome_to_fragmentEdit"
            app:destination="@id/fragmentEdit" />
        <action
            android:id="@+id/action_fragmentHome_to_fragmentList"
            app:destination="@id/fragmentList" />
        <action
            android:id="@+id/action_goto_map"
            app:destination="@id/mapFragment" />
    </fragment>
    <fragment
        android:id="@+id/fragmentEdit"
        android:name="elfak.mosis.myplaces.FragmentEdit"
        android:label="fragment_edit"
        tools:layout="@layout/fragment_edit" >
        <action
            android:id="@+id/action_fragmentEdit_to_fragmentHome"
            app:destination="@id/fragmentHome" />
        <action
            android:id="@+id/action_fragmentEdit_to_fragmentList"
            app:destination="@id/fragmentList" />
        <action
            android:id="@+id/action_edit_to_map"
            app:destination="@id/mapFragment" />
    </fragment>
    <fragment
        android:id="@+id/fragmentList"
        android:name="elfak.mosis.myplaces.FragmentList"
        android:label="fragment_list"
        tools:layout="@layout/fragment_list" >
        <action
            android:id="@+id/action_fragmentList_to_fragmentEdit"
            app:destination="@id/fragmentEdit" />
        <action
            android:id="@+id/action_fragmentList_to_fragmentHome"
            app:destination="@id/fragmentHome" />
        <action
            android:id="@+id/action_fragmentList_to_fragmentView"
            app:destination="@id/fragmentView" />
        <action
            android:id="@+id/action_list_to_map"
            app:destination="@id/mapFragment" />
    </fragment>
    <fragment
        android:id="@+id/fragmentView"
        android:name="elfak.mosis.myplaces.FragmentView"
        android:label="fragment_view"
        tools:layout="@layout/fragment_view" >
        <action
            android:id="@+id/action_fragmentView_to_fragmentList"
            app:destination="@id/fragmentList" />
    </fragment>
    <fragment
        android:id="@+id/mapFragment"
        android:name="elfak.mosis.myplaces.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" >
        <action
            android:id="@+id/action_map_to_edit"
            app:destination="@id/fragmentEdit" />
    </fragment>
</navigation>